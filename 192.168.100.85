cd ~/.local/share/fonts 
wget https://github.com/ryanoasis/nerd-fonts/releases/download/v2.3.3/CascadiaCode.zip 
unzip CascadiaCode.zip  
rm CascadiaCode.zip 
rm readme.md 
rm LICENSE 
fc-cache ~/.local/share/fonts
source ~/.bashrc

